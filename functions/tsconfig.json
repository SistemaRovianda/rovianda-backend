{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6",
        "esnext.asynciterable"
     ],
     "plugins": [
      {
      "transform": "ts-transformer-keys/transformer"
      }
     ],
     "target": "es5",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "./build",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "strict": false,
     "esModuleInterop": false,
     "downlevelIteration":true
  }
}